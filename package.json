{
  "name": "blis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android:dev": "ENVFILE=.env.development react-native run-android",
    "android:prod": "ENVFILE=.env.production react-native run-android",
    "android:apk:dev": "cd android && ENVFILE=.env.development ./gradlew assembleRelease",
    "android:apk:prod": "cd android && ENVFILE=.env.production ./gradlew assembleRelease",
    "rm-env-cache": "rm -f node_modules/react-native-config/ios/ReactNativeConfig/GeneratedDotEnv.m",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --continue -- --fix",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --write",
    "format:fix": "yarn format --write",
    "type-check": "tsc --noEmit",
    "postinstall": "pod-install"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@react-native-google-signin/google-signin": "^9.0.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.11",
    "@tanstack/react-query": "^4.35.3",
    "add": "^2.0.6",
    "axios": "^1.2.3",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "eslint-plugin-prettier": "^5.0.0",
    "lodash": "^4.17.21",
    "nativewind": "^2.0.11",
    "postcss": "^8.4.29",
    "prop-types": "^15.8.1",
    "react": "18.1.0",
    "react-hook-form": "^7.42.1",
    "react-native": "0.70.6",
    "react-native-config": "^1.5.0",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-heroicons": "^3.2.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-onboarding-swiper": "^1.2.0",
    "react-native-outside-press": "^1.1.0",
    "react-native-progress": "^5.0.1",
    "react-native-quick-base64": "^2.0.5",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.18.2",
    "react-native-svg": "^13.7.0",
    "react-native-uuid": "^2.0.1",
    "superjson": "^1.12.2",
    "zod": "^3.20.2",
    "zustand": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^3.0.2",
    "@types/eslint": "^8.44.2",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "clsx": "^2.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
